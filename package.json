{
  "version": "0.0.42",
  "license": "MIT",
  "main": "dist/sdk.esm.js",
  "browser": "dist/sdk.esm.js",
  "typings": "dist/index.d.ts",
  "type": "module",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=18"
  },
  "scripts": {
    "start": "tsdx watch --format esm",
    "build": "tsdx build --format esm",
    "test": "tsdx test",
    "lint": "tsdx lint",
    "prepare": "tsdx build",
    "size": "size-limit",
    "analyze": "size-limit --why"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "name": "@jsdb/sdk",
  "author": "jpcapdevila",
  "module": "dist/sdk.esm.js",
  "size-limit": [
    {
      "path": "dist/sdk.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/sdk.esm.js",
      "limit": "10 KB"
    }
  ],
  "devDependencies": {
    "@size-limit/preset-small-lib": "^7.0.8",
    "@types/lodash-es": "^4.17.6",
    "husky": "^7.0.4",
    "size-limit": "^7.0.8",
    "tsdx": "^0.14.1",
    "tslib": "^2.4.0",
    "typescript": "^3.9.10"
  },
  "dependencies": {
    "isomorphic-ws": "^4.0.1",
    "ws": "^8.5.0"
  },
  "description": "Install ```shell npm i @jsdb/sdk ```",
  "directories": {
    "test": "test"
  }
}
